server {
    listen 8080;
    root /data/myweb/www;
    index index.html index.htm;
    
    location / {
        try_files $uri $uri/ =404;
    }

     location {{ forbidden_403 }} {
        return 403;
    }
}

server {
    location / {
        proxy_pass http://10.200.10.40:8080;
            # root /data/myweb/www;
    }
    location ~ \.(gif|jpg|png)$ {
        root /data/images;
    }
}